<form onsubmit="if(!confirm('Esta seguro de eliminar este registro ?'))event.preventDefault()" style="display:inline" method="post" action="{{ path('doctor_delete', {'id': doctor.id}) }}" >
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ doctor.id) }}">
    <button class="btn btn-danger">Eliminar</button>
</form>
